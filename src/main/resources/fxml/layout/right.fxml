<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:id="content" fx:controller="com.example.labjavafx.controller.layout.RightController">
    <children>
        <Label text="BIENVENIDO">
            <font>
                <Font size="24.0"/>
            </font>
            <effect>
                <SepiaTone/>
            </effect>
        </Label>
        <Separator prefWidth="200.0"/>
        <ImageView fitHeight="400" fitWidth="400" pickOnBounds="true" preserveRatio="true">
            <Image url="@images/hello.jpg"/>
        </ImageView>
    </children>
</VBox>
